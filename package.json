{
  "name": "doublek-ui",
  "version": "0.0.11",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm clean && vue-tsc && vite build && pnpm type",
    "preview": "vite preview",
    "type": "node ./src/scripts/buildType.cjs",
    "test": "vitest",
    "clean": "rm -rf dist",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-c8": "^0.29.8",
    "@vue/test-utils": "^2.3.2",
    "happy-dom": "^8.9.0",
    "lodash-es": "^4.17.21",
    "postcss-mixins": "^9.0.4",
    "postcss-rem": "^2.0.4",
    "rollup-plugin-delete": "^2.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.8",
    "vue-tsc": "^1.2.0",
    "vuepress": "^2.0.0-beta.45"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/doublek-ui.umd.js",
  "module": "./dist/doublek-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/doublek-ui.es.js",
      "require": "./dist/doublek-ui.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}
