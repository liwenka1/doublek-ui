{
  "name": "doublek-ui",
  "version": "0.0.14",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm clean && vue-tsc && vite build && pnpm type",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "prettier": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
    "preview": "vite preview",
    "type": "node ./src/scripts/buildType.cjs",
    "test": "vitest",
    "clean": "rm -rf dist",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-c8": "^0.29.8",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.0",
    "happy-dom": "^8.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "lodash-es": "^4.17.21",
    "postcss-mixins": "^9.0.4",
    "postcss-rem": "^2.0.4",
    "prettier": "^2.8.8",
    "rollup-plugin-delete": "^2.0.0",
    "sass": "^1.55.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.8",
    "vue-tsc": "^1.2.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/doublek-ui.umd.js",
  "module": "./dist/doublek-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/doublek-ui.es.js",
      "require": "./dist/doublek-ui.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run eslint",
      "npm run prettier"
    ]
  }
}
